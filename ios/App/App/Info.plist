			</array>
		</dict>
	</array>
	
	<!-- App Category -->
	<key>LSApplicationCategoryType</key>
	<string>public.app-category.social-networking</string>
	
	<!-- Minimum iOS Version -->
	<key>MinimumOSVersion</key>
	<string>13.0</string>
	
	<!-- Background Modes (if needed) -->
	<key>UIBackgroundModes</key>
	<array>
		<string>background-fetch</string>
