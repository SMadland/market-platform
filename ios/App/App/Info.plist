	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<false/>
		<key>NSExceptionDomains</key>
		<dict>
			<key>azmrpbfifkznhqbyftsg.supabase.co</key>
			<dict>
				<key>NSExceptionAllowsInsecureHTTPLoads</key>
				<false/>
				<key>NSExceptionMinimumTLSVersion</key>
				<string>TLSv1.2</string>
			</dict>
			<key>mamon.no</key>
			<dict>
				<key>NSExceptionAllowsInsecureHTTPLoads</key>
				<false/>
				<key>NSExceptionMinimumTLSVersion</key>
				<string>TLSv1.2</string>
			</dict>
			<!-- Development only -->
			<key>localhost</key>
			<dict>
				<key>NSExceptionAllowsInsecureHTTPLoads</key>
				<true/>
			</dict>
		</dict>
	</dict>
	
	<!-- Permissions -->
	<key>NSCameraUsageDescription</key>
	<string>Appen trenger tilgang til kameraet for å ta bilder av produkter du vil anbefale.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>Appen trenger tilgang til fotobiblioteket for å velge bilder av produkter.</string>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>Appen trenger tilgang til å lagre bilder i fotobiblioteket.</string>
	
